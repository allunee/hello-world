                                                <tr>
                                                        <th>Price</th>
                                                        <th>Amount</th>
                                                        <th>Total</th>
                                                        <th>Sum</th>
                                                </tr>
                                        </thead>
                                        <tbody></tbody>
                                </table>
                        </div>
                </div>
                <div class="col-xs-12 col-sm-6">
                        <h2 class="subsection-header-in-row">Asks</h2>
                        <div class="left-table-wrapper">
                                <table id="buyOrders" class="orderbook">
                                        <thead>
                                                <tr>
                                                        <th>Price</th>
                                                        <th>Amount</th>
                                                        <th>Total</th>
                                                        <th>Sum</th>
                                                </tr>
                                        </thead>
                                        <tbody></tbody>
                                </table>
                        </div>
                </div>
        </div>

        <div class="row" ng-controller="tradeHistoryCtl" >
                <div class="col-xs-12" id="orderSummary">
                        <div class="row">
                                <div class="col-xs-4">
                                        <h2 class="section-header-in-row">Market History</h2>
                                </div>
                                <div class="col-xs-8">
                                        <div id="market-toolbar-history" class="table-toolbar table-toolbar-in-header">
                                        <div class="dataTables_length" id="marketHistoryTable2_length2" style="float:right">
                                                <label>
                                                <div style="padding-top: 5px; padding-right: 4px; float: left;">Display&nbsp;</div>
                                                <select class="form-control input-sm" style="float: left;width:70px" data-bind="options: history.availableLength, optionsText: 'value', value: history.selectedLength, optionsCaption: 'Choose...'"><option value="">Choose...</option><option value="">10</option><option value="">25</option><option value="">50</option><option value="">100</option></select>
                                                <div style="padding-top: 5px; padding-right: 0px; padding-left: 4px; float: left;">&nbsp;rows</div>
                                                </label>
                                        </div>
                                        </div>    
                                </div>
                        </div>
                        <div class="right-table-wrapper">
                                <table id="tradeHistory" class="orderbook">
                                        <thead>
                                                <tr>
                                                        <th>Date</th>
                                                        <th>Type</th>
                                                        <th>Rate</th>
                                                        <th>Amount</th>
                                                        <th>Total</th>
                                                </tr>
                                        </thead>
                                        <tbody></tbody>
                                </table>
                        </div>
                </div>
